{"uwsgi": {
        "close-on-exec": true,
        "enable-threads": true,
        "master": true,
        "max-requests": 5000,
        "module": "fig_demo.wsgi",
        "cheaper-step": 1,
        "harakiri": 60,
        "env": "DJANGO_SETTINGS_MODULE=fig_demo.settings",
        "cheaper-initial": 0,
        "cheaper-busyness-verbose": true,
        "chdir": "/var/www/fig-demo/fig_demo",
        "cheaper-busyness-min": 50,
        "workers": 4,
        "cheaper-busyness-backlog-alert": 40,
        "harakiri-verbose": true,
        "cheaper-busyness-max": 90,
        "cheaper-overload": 10,
        "cheaper": 1,
        "pythonpath": "/var/www/fig-demo/",
        "home": "/var/www/env",
        "cheaper-busyness-multiplier": 5,
        "uwsgi-socket": ":8000",
        "buffer-size": 65535,
}}
